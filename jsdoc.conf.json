{
    "source": {
        "include": ["."],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "opts":{
        "encoding": "utf8",
        "destination": "./docs/",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        }
    }
}